### Stripping modules
echo "Stripping modules"
cd final_files/
for i in $(find . | grep .ko | grep './')
do
        echo $i
	/home/doomlord/android/system/prebuilt/linux-x86/toolchain/arm-eabi-4.4.3/bin/arm-eabi-strip --strip-unneeded $i
done

