/cache      ext4    /dev/block/stl11
/data       ext4    /dev/block/mmcblk0p3
/system     ext4    /dev/block/stl9
/dbdata     ext4    /dev/block/stl10
/sdcard     vfat    /dev/block/mmcblk1p1
/emmc       vfat    /dev/block/mmcblk0p1

